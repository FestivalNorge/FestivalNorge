{
  "name": "festivalnorge",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:scripts": "tsc -b tsconfig.scripts.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "init:festivals": "node dist/scripts/init-festivals.js",
    "upload:festival": "node dist/scripts/uploadFestivals.js",
    "upload:festivals": "node scripts/upload-festivals.cjs",
    
    "emulate": "firebase emulators:start",
    "preview:deploy": "firebase hosting:channel:deploy preview-$(git rev-parse --short HEAD) --expires 7d",
    "deploy:staging": "firebase deploy -P staging",
    "predeploy": "npm run build",
    "deploy": "firebase deploy",
    "deploy:prod": "npm run build && firebase deploy -P production"
  },
  "dependencies": {
    "@firebase/analytics": "^0.10.16",
